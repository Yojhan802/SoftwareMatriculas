	<parameter name="periodo" class="java.lang.String"/>
	<queryString language="SQL">
		<![CDATA[SELECT 
    m.nivel,
    m.grado,
    a.dni_alumno,
    CONCAT(a.apellido, ', ', a.nombre) as nombre_completo,
    m.fecha_matricula,
    m.periodo
FROM matricula m
INNER JOIN alumno a ON m.id_alumno = a.id_alumno
WHERE m.estado = 'Activo' 
  AND m.periodo = $P{periodo}
ORDER BY m.nivel, m.grado, a.apellido]]>
	</queryString>
